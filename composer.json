{
  "name": "godie/overphp",
  "description": "Minimal PHP API framework",
  "type": "project",
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "OverPHP\\": "src/"
    },
    "files": [
      "src/Helpers/cors.php",
      "src/Helpers/database.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "OverPHP\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --configuration phpunit.xml"
  }
}
